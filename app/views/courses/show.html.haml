-provide :title, "#{Project.find(@course.project_id).name}"
.container
  .row
    .col-md-3
      Member Projects
    .col-md-9
      .project-container
        =stylesheet_link_tag 'projects', media: 'all'
        %ul{style: "list-style:none;"}
          -@course.projects.each do |project|
            =render "/shared/project_comb", locals: {project: project}